<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="@android:color/transparent">

    <TextView
        android:id="@+id/firstmission1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:fontFamily="@font/dunggeunmo"
        android:text="100보 걷기"
        android:textColor="@color/white"
        android:textSize="25dp"
        tools:layout_editor_absoluteX="30dp"
        tools:layout_editor_absoluteY="100dp" />

    <TextView
        android:id="@+id/onecoin"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:fontFamily="@font/dunggeunmo"
        android:text="1코인"
        android:textColor="@color/white"
        android:textSize="25dp"
        tools:layout_editor_absoluteX="180dp"
        tools:layout_editor_absoluteY="100dp"/>

    <ImageView
        android:id="@+id/checkbox11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/checkbox_x"
        android:translationX="270dp"
        android:translationY="90dp"/>

    <TextView
        android:id="@+id/secondmission1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:fontFamily="@font/dunggeunmo"
        android:text="500보 걷기"
        android:textColor="@color/white"
        android:textSize="25dp"
        tools:layout_editor_absoluteX="30dp"
        tools:layout_editor_absoluteY="160dp" />

    <TextView
        android:id="@+id/fivecoin"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:fontFamily="@font/dunggeunmo"
        android:text="5코인"
        android:textColor="@color/white"
        android:textSize="25dp"
        tools:layout_editor_absoluteX="180dp"
        tools:layout_editor_absoluteY="160dp"   />

    <ImageView
        android:id="@+id/checkbox12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/checkbox_x"
        android:translationX="270dp"
        android:translationY="150dp"/>

    <TextView
        android:id="@+id/thirdmission1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:fontFamily="@font/dunggeunmo"
        android:text="1000보 걷기"
        android:textColor="@color/white"
        android:textSize="25dp"
        tools:layout_editor_absoluteX="25dp"
        tools:layout_editor_absoluteY="220dp" />

    <TextView
        android:id="@+id/twentycoin"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:fontFamily="@font/dunggeunmo"
        android:text="20코인"
        android:textColor="@color/white"
        android:textSize="25dp"
        tools:layout_editor_absoluteX="175dp"
        tools:layout_editor_absoluteY="220dp"   />

    <ImageView
        android:id="@+id/checkbox13"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/checkbox_x"
        android:translationX="270dp"
        android:translationY="210dp"/>

    <TextView
        android:id="@+id/fourthmission1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:fontFamily="@font/dunggeunmo"
        android:text="5000보 걷기"
        android:textColor="@color/white"
        android:textSize="25dp"
        tools:layout_editor_absoluteX="25dp"
        tools:layout_editor_absoluteY="280dp" />

    <TextView
        android:id="@+id/sixtycoin"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:fontFamily="@font/dunggeunmo"
        android:text="60코인"
        android:textColor="@color/white"
        android:textSize="25dp"
        tools:layout_editor_absoluteX="175dp"
        tools:layout_editor_absoluteY="280dp"   />

    <ImageView
        android:id="@+id/checkbox14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/checkbox_x"
        android:translationX="270dp"
        android:translationY="270dp"/>

    <TextView
        android:id="@+id/fifthmission1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:fontFamily="@font/dunggeunmo"
        android:text="10000보 걷기"
        android:textColor="@color/white"
        android:textSize="25dp"
        tools:layout_editor_absoluteX="20dp"
        tools:layout_editor_absoluteY="340dp" />

    <TextView
        android:id="@+id/onehundredcoin"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:fontFamily="@font/dunggeunmo"
        android:text="120코인"
        android:textColor="@color/white"
        android:textSize="25dp"
        tools:layout_editor_absoluteX="170dp"
        tools:layout_editor_absoluteY="340dp"   />

    <ImageView
        android:id="@+id/checkbox15"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/checkbox_x"
        android:translationX="270dp"
        android:translationY="330dp"/>
</androidx.constraintlayout.widget.ConstraintLayout>